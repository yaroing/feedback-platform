# Configuration du webhook JSON SMS

# URL de l'API de la plateforme de feedback
FEEDBACK_URL=http://backend:8000/api/webhook/json-sms/

# Clé API pour l'authentification (optionnel)
API_KEY=your_api_key_here

# Vérification SSL pour les requêtes vers la plateforme de feedback (true/false)
VERIFY_SSL=false

# Configuration du journal
LOG_LEVEL=INFO
LOG_FILE=webhook.log

# Configuration du serveur
HOST=0.0.0.0
PORT=5000
